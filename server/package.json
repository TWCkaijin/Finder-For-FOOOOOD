{
    "name": "gourmet-finder-server",
    "version": "1.0.0",
    "description": "Server for Gourmet Finder AI",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "LOCAL_DEV=true tsx watch src/index.ts",
        "build": "tsc",
        "deploy": "firebase deploy --only functions",
        "deploy:env": "tsx scripts/deploy-with-env.ts",
        "serve": "npm run build && firebase emulators:start"
    },
    "dependencies": {
        "@genkit-ai/firebase": "^1.26.0",
        "@genkit-ai/google-genai": "^1.26.0",
        "@google/genai": "^1.34.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.18.2",
        "firebase": "^12.7.0",
        "firebase-admin": "^12.0.0",
        "firebase-functions": "^7.0.2",
        "genkit": "^1.26.0",
        "zod": "^4.2.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.19",
        "tsx": "^4.7.1",
        "typescript": "^5.3.3"
    },
    "pnpm": {
        "overrides": {
            "node-domexception": "file:./patches/node-domexception"
        }
    }
}